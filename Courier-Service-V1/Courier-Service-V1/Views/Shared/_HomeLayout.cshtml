@{
    string RiderId = Context.Request.Cookies["RiderId"];
    string AdminId = Context.Request.Cookies["AdminId"];
    string MerchantId = Context.Request.Cookies["MerchantId"];
    string AdminEmail = Context.Request.Cookies["AdminEmail"];
    string RiderEmail = Context.Request.Cookies["RiderEmail"];
    string MerchantEmail = Context.Request.Cookies["MerchantEmail"];


}


<!DOCTYPE html>
<html lang="en">
<head>
    <title>Flyer Express</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Oswald:400,700|Dancing+Script:400,700|Muli:300,400" rel="stylesheet">
    <link rel="stylesheet" href="~/home/fonts/icomoon/style.css">
    <!-- App favicon -->
    <link rel="shortcut icon" href="https://i.ibb.co/nrbyqDb/cropped-20240212-042330-0000-1.webp?fbclid=IwAR1NZ_esF6HEjndt-7mpQEAgvTV_ptZlr3n0COhZFFAu4DzeOkLipq7P0ik">

    <link rel="stylesheet" href="~/home/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/home/css/jquery-ui.css">
    <link rel="stylesheet" href="~/home/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/home/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/home/css/owl.theme.default.min.css">

    <link rel="stylesheet" href="~/home/css/jquery.fancybox.min.css">

    <link rel="stylesheet" href="~/home/css/bootstrap-datepicker.css">

    <link rel="stylesheet" href="~/home/fonts/flaticon/font/flaticon.css">

    <link rel="stylesheet" href="~/home/css/aos.css">
    <link href="~/home/css/jquery.mb.YTPlayer.min.css" media="all" rel="stylesheet" type="text/css">

    <link rel="stylesheet" href="~/home/css/style.css">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css">



</head>
<body>


    <div class="site-wrap">
        <div class="site-mobile-menu site-navbar-target">
            <div class="site-mobile-menu-header">
                <div class="site-mobile-menu-close mt-3">
                    <span class="icon-close2 js-menu-toggle"></span>
                </div>
            </div>
            <div class="site-mobile-menu-body"></div>
        </div>



        <div class="header-top bg-light">
            <div class="container">
                <div class="row align-items-center justify-content-between">
                    <div class="col-6 col-lg-2">
                        <a asp-action="Home">
                            <img src="https://i.ibb.co/nrbyqDb/cropped-20240212-042330-0000-1.webp?fbclid=IwAR1NZ_esF6HEjndt-7mpQEAgvTV_ptZlr3n0COhZFFAu4DzeOkLipq7P0ik" alt="Image" class="img-fluid">
                            <!-- <strong>Water</strong>Boat -->
                        </a>
                    </div>
                    <div class="col-lg-4 d-none d-lg-block">

                        <div class="quick-contact-icons d-flex">
                            <div class="icon align-self-start">
                                <span class="icon-location-arrow text-primary"></span>
                            </div>
                            <div class="text">
                                <span class="h4 d-block">House 267/1/A, 2rd Floor, 60 Feet Main Road,South Pirerbag</span>
                                <span class="caption-text">Dhaka,Bangladesh</span>
                            </div>
                        </div>

                    </div>
                    <div class="col-lg-2 d-none d-lg-block">
                        <div class="quick-contact-icons d-flex">
                            <div class="icon align-self-start">
                                <span class="icon-phone text-primary"></span>
                            </div>
                            <div class="text mt-2">
                                <span class="h4 d-block">01701-018080</span>
                                
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-3 d-none d-lg-block">
                        <div class="quick-contact-icons d-flex align-items-center">
                            <div class="icon align-self-start">
                                <span class="icon-envelope text-primary"></span>
                            </div>
                            <div class="text">
                                <span class="h4 d-block">hello@flyerbd.com</span>
                                
                            </div>
                        </div>
                    </div>

                    <div class="col-6 d-block d-lg-none text-right">
                        <a href="#" class="d-inline-block d-lg-none site-menu-toggle js-menu-toggle text-black">
                            <span class="icon-menu h3"></span>
                        </a>
                    </div>
                </div>
            </div>




            <div class="site-navbar bg-primary py-2 js-sticky-header site-navbar-target d-none pl-0 d-lg-block" role="banner">

                <div class="container">
                    <div class="d-flex align-items-center">

                        <div class="mx-auto">
                            <nav class="site-navigation position-relative text-right" role="navigation">
                                <ul class="site-menu main-menu js-clone-nav mr-auto d-none pl-0 d-lg-block">
                                    <li class="active">
                                        <a asp-action="Home" class="nav-link text-left">Home</a>
                                    </li>
                                    <li>
                                        <a href="#About" class="nav-link text-left">About Us</a>
                                    </li>
                                    <li>
                                        <a href="#Service" class="nav-link text-left">Services</a>
                                    </li>
                                    
                                    <li>
                                        <a href="#Contact" class="nav-link text-left">Contact</a>
                                    </li>
                                    @if(AdminEmail != null)
                                    {
                                        <li>
                                            <a asp-action="Index"  class="nav-link text-left">@AdminEmail</a>
                                        </li>
                                        <li>
                                            <a asp-action="Logout" asp-controller="Home" class="nav-link text-left">Logout</a>
                                        </li>
                                    }
                                    else if(RiderEmail != null)
                                    {
                                        <li>
                                            <a asp-action="Index" asp-controller="Rider" class="nav-link text-left">@RiderEmail</a>
                                        </li>
                                        <li>
                                            <a asp-action="Logout" asp-controller="Home" class="nav-link text-left">Logout</a>
                                        </li>
                                    }
                                    else if(MerchantEmail != null)
                                    {
                                        <li>
                                            <a asp-action="Index" asp-controller="Merchant" class="nav-link text-left">@MerchantEmail</a>
                                        </li>
                                        <li>
                                            <a asp-action="Logout" asp-controller="Home" class="nav-link text-left">Logout</a>
                                        </li>
                                    }
                                    else
                                    {
                                        <li>
                                            <a asp-action="Login" class="nav-link text-left">Login</a>
                                        </li>
                                    }
                                   
                                </ul>                                                                                                                                                                                                                                                                                          </ul>
                            </nav>

                        </div>

                    </div>
                </div>

            </div>

        </div>
    </div>

    

    <div class="">
        <main role="main" class="">
            <partial name="_Notification" />
            @RenderBody()
        </main>
    </div>

   
    <!-- Footer Start -->
    <div class="footer bg-light">
        <div class="container">
            <div class="row">
                <div class="col-lg-3">
                    <p class="mb-4"><img src="https://i.ibb.co/nrbyqDb/cropped-20240212-042330-0000-1.webp?fbclid=IwAR1NZ_esF6HEjndt-7mpQEAgvTV_ptZlr3n0COhZFFAu4DzeOkLipq7P0ik" alt="Your Courier Service Logo" class="img-fluid"></p>
                    <p>Your trusted partner for fast and reliable courier services. We deliver excellence, every time.</p>
                    
                </div>
                <div class="col-lg-3">
                    <h3 class="footer-heading"><span>Our Company</span></h3>
                    <ul class="list-unstyled">
                        <li><a href="#About">About Us</a></li>
                        <li><a href="#Service">Services</a></li>
                        <li><a href="#Contact">Contact Us</a></li>
                    </ul>
                </div>
                <div class="col-lg-3">
                    <h3 class="footer-heading"><span>Our Services</span></h3>
                    <ul class="list-unstyled">
                        <li><a href="#">In Side Dhaka</a></li>
                        <li><a href="#">Sub Dhaka</a></li>
                        <li><a href="#">Out Side Dhaka</a></li>
                        <li><a href="#">In Dhaka Emergency</a></li>
                        <li><a href="#">P2P Parcel</a></li>
                    </ul>
                </div>
                <div class="col-lg-3">
                    <h3 class="footer-heading"><span>Contact</span></h3>
                    <ul class="list-unstyled">
                        <li><a href="#">Customer Support</a></li>
                        <li><a href="#">FAQs</a></li>
                        <li><a href="#">Track Your Shipment</a></li>
                        <li><a href="#">Become a Merchant</a></li>
                    </ul>
                </div>
            </div>

            <div class="row">
                <div class="col-12">
                    <div class="copyright">
                        <p>
                            &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved | Powered by <a target="_blank" href="https://techenovative.com/">Tech Enovative</a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Footer -->
    <!-- loader -->
    <div id="loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#ff5e15" /></svg></div>

    <script src="~/home/js/jquery-3.3.1.min.js"></script>
    <script src="~/home/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/home/js/jquery-ui.js"></script>
    <script src="~/home/js/popper.min.js"></script>
    <script src="~/home/js/bootstrap.min.js"></script>
    <script src="~/home/js/owl.carousel.min.js"></script>
    <script src="~/home//jquery.stellar.min.js"></script>
    <script src="~/home/js/jquery.countdown.min.js"></script>
    <script src="~/home/js/bootstrap-datepicker.min.js"></script>
    <script src="~/home/js/jquery.easing.1.3.js"></script>
    <script src="~/home/js/aos.js"></script>
    <script src="~/home/js/jquery.fancybox.min.js"></script>
    <script src="~/home/js/jquery.sticky.js"></script>
    <script src="~/home/js/jquery.mb.YTPlayer.min.js"></script>




    <script src="~/home/js/main.js"></script>


    <!-- Vendor js -->
    <script src="~/assets/js/vendor.min.js"></script>

    <!--Morris Chart-->
    <script src="~/assets/libs/morris-js/morris.min.js"></script>
    <script src="~/assets/libs/raphael/raphael.min.js"></script>

    <!-- Dashboard init js-->
    <script src="~/assets/js/pages/dashboard.init.js"></script>

    <!-- App js -->
    <script src="~/assets/js/app.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="//cdn.datatables.net/2.0.0/js/dataTables.min.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
